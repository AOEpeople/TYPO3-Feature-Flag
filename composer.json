{
    "name": "aoe/feature-flag",
    "description": "Add ability to use feature flags for extensions and content elements.",
    "type": "typo3-cms-extension",
    "homepage": "https://forge.typo3.org/projects/extension-feature_flag",
    "license": "GPL-3.0",
    "support": {
        "email": "dev@aoe.com",
        "issues": "https://forge.typo3.org/projects/extension-feature_flag/issues",
        "source": "https://forge.typo3.org/projects/extension-feature_flag"
    },
    "authors": [
        {
            "name": "AOE GmbH",
            "email": "dev@aoe.com",
            "homepage": "http://www.aoe.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "typo3/cms-core": ">=6.2.0, <=7.6.99",
        "typo3/cms-extbase": "*",
        "typo3/cms-scheduler": "*"
    },
    "require-dev": {
        "typo3/cms": "^6.2",
        "nimut/testing-framework": "2.0.0",
        "phpunit/phpcov": "2.0.*",
        "squizlabs/php_codesniffer": "2.3.*",
        "sebastian/phpcpd": "2.0.*",
        "phpmd/phpmd": "2.2.*"
    },
    "replace": {
        "feature_flag": "*"
    },
    "autoload": {
        "classmap": [
            "Classes/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "Tests"
        ]
    },
    "keywords": [
        "TYPO3",
        "extbase",
        "feature flag"
    ]
}